{
    "name": "ivy-syntax-highlight",
    "displayName": "Ivy-Syntax-Highlight",
    "description": "Syntax highlight support for Ivy",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
		"name": "Roberto Saltini",
		"email": "roberto.saltini@consensys.net"
	},
    "publisher": "roberto-saltini",
	"license": "Apache 2.0",
	"homepage": "https://github.com/ConsenSys/ivy-syntax-highlight",
	"bugs": "https://github.com/ConsenSys/ivy-syntax-highlight",
	"repository": {
		"type": "git",
		"url": "https://github.com/ConsenSys/ivy-syntax-highlight"
	},    
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ivy",
            "aliases": ["Ivy", "ivy"],
            "extensions": [".ivy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ivy",
            "scopeName": "source.ivy",
            "path": "./syntaxes/ivy.tmLanguage.json"
        }]
    }
}